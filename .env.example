# Tavily API Key - Get yours at https://tavily.com
TAVILY_API_KEY=your_tavily_api_key_here

# OpenAI API Key - Get yours at https://platform.openai.com
OPENAI_API_KEY=your_openai_api_key_here

# Logfire Token - Get yours at https://logfire.pydantic.dev
# Run `logfire auth` locally or get from https://logfire.pydantic.dev/
LOGFIRE_TOKEN=your_logfire_token_here

# Optional: Flask Configuration
FLASK_ENV=development

# Optional: API URL for frontend (used in production)
API_URL=http://localhost:8000
